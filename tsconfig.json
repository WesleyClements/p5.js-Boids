{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "rootDir": "src",
    "outDir": "build",
    "sourceRoot": "./src/",
    "inlineSourceMap": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "preserveConstEnums": true,
    "removeComments": true
  }
}
